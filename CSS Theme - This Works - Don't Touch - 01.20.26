/*Rules for revealing the scroll content, and positioning*/

@media only screen and (max-width: 990px) {
.vertisimplescroll {
    left: -35px !important;
    transform: scale(0.9);}
.user-css .panel .vertisimplescroll {
    left: -50px !important;
    transform: scale(0.9);}
.user-css .col-md-6 .vertisimplescroll {
    left: -50px !important;
    transform: scale(0.9);}
}


.vertisimplescroll {
    position: relative;
    left: 0px;
    top: 0px;
    margin-left: 0px;
    margin-top: 0px;
    margin-bottom: 500px;
}

.vertisimplescroll .spoiler-card {
    display: none;
}

.user-css .panel .vertisimplescroll {
    left: 10px;
}

.user-css .col-md-6 .vertisimplescroll {
    left: 10px;
}

@keyframes scrolltext {
0% {display: block;
    opacity: 0%;}
10% {text-shadow: 0 0 0 white;}

30% {text-shadow: 0px 0px 50px white, 0px 0px 30px white;}

60% {text-shadow: 0px 0px 20px white;}

100% {
    display: block;
    opacity: 100%;}}

.vertisimplescroll {
    display: block;
}

.vertisimplescroll .well,
.vertisimplescroll .collapse.in,
.vertisimplescroll .collapsing {
    display: none;
}

.vertisimplescroll .spoiler-button[aria-expanded$=true] ~ .scrollcontent {
    animation: scrolltext;
    text-shadow: 0px 0px 15px white;
    display: block;
    transition: all 1.9s;
    height: 370px;
}
.vertisimplescroll .scrollcontent {
    position: absolute;
    z-index: 1;
    left: 32px;
    top: 40px;
    width: 290px;
    height: 0px;
    padding-top: 20px;
    padding-left: 0px;
    padding-right: 30px;
    transition: all 2.2s;
    overflow: auto;
}

.vertisimplescroll .spoiler-button[aria-expanded$=true] ~ .scrollbg {
    height: 450px;
    transition: 2s;
}
.vertisimplescroll .scrollbg {
    width: 310px;
    height: 50px;
    top: 0px;
    left: 20px;
    position: absolute;
    background: linear-gradient(
          rgba(0, 0, 0, 0.0), 
          rgba(0, 0, 0, 0.3)
        ),
        url(https://www.worldanvil.com/uploads/images/b15f567be454e48f739dbcfefef138fc.png);
    transition: 2s;
}


/*Spoiler button styling rules, including what happesn when the button is hovered, and then clicked on*/
.vertisimplescroll .spoiler-button {
    position: absolute;
    transform: translateZ(80px);
    margin: 0px;
    left: 20px;
    top: 33px;
