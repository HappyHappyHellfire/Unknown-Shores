
/* ==============================
     CSS THEME BY IMAGICA (aka Make lore, not war)
   RUINED BY: SOLOMONJACK
============================== */


/* ==============================
   FONTS: EMBEDDED FONT FILES
============================== */

@font-face {
  font-family: Jim Nightshade;
  font-style: normal;
  font-weight: 400;
  font-display: swap;
  src: url(https://fonts.gstatic.com/s/jimnightshade/v20/PlIkFlu9Pb08Q8HLM1PxmB0g-NS_XXs.woff2);
}

@font-face {
  font-family: Cardo;
  font-style: normal;
  font-weight: 400;
  font-display: swap;
  src: url(https://fonts.gstatic.com/s/cardo/v20/wlp_gwjKBV1pqhv43IE.woff2);
}

@font-face {
  font-family: Island Moments;
  font-style: normal;
  font-weight: 400;
  font-display: swap;
  src: url(https://fonts.gstatic.com/s/islandmoments/v7/NaPBcZfVGvBdxIt7Ar0qzkXJF9TBKIk.woff2);
}

@font-face {
  font-family: Montserrat;
  font-style: normal;
  font-weight: 100 900;
  font-display: swap;
  src: url(https://fonts.gstatic.com/s/montserrat/v30/JTUSjIg1_i6t8kCHKm459Wlhyw.woff2);
}


/* ==============================
   ROOT VARIABLES
============================== */

:root {
  /* FONTS */
  --font-header: Jim Nightshade, Georgia, Times New Roman, sans-serif;
  --font-body: Cardo, Garamond, serif;
  --font-quote: Island Moments, cursive;
  --font-meta: Montserrat, Arial, Helvetica, sans-serif;
  --font-text: var(--font-body);

  /* BACKGROUNDS */
  --color-background: #04090F;
  --color-background-gradient: linear-gradient(
    180deg,
    rgba(4, 9, 15, 1) 0%,
    rgba(12, 30, 35, 1) 50%,
    rgba(9, 42, 47, 1) 100%
  );
  --color-secondary-bg: #09121A;
  --color-secondary-gradient: linear-gradient(
    180deg,
    rgba(9, 18, 26, 1) 0%,
    rgba(14, 30, 38, 1) 100%
  );

  /* TEXT */
  --color-text: #E6E1D8;
  --color-muted: #BEB28F;

  /* LINKS & HIGHLIGHTS */
  --color-link: #C68C53;
  --color-link-hover: linear-gradient(45deg, #C68C53, #FFDCC1, #E07B21);
  --color-header: linear-gradient(
    45deg,
    #4A2C1A 15%,
    #A85C32 35%,
    #D98E5F 55%,
    #A85C32 75%,
    #4A2C1A 85%
  );
  --color-small-header: linear-gradient(
    45deg,
    #4A2C1A 0%,
    #A85C32 10%,
    #D98E5F 55%,
    #A85C32 95%,
    #4A2C1A 100%
  );
  --color-border: #B3744D;
  --color-shadow: rgba(224, 180, 120, 0.4);
  --color-header-shadow: rgba(0, 0, 0, 0.3) 1px 1px 1px;
  --color-accent: #D9A76F;

  --author-css: Imagica;
}


/* ==============================
   GLOBAL ANIMATIONS
============================== */

@keyframes fadeInFromBlack {
  0% {
    opacity: 0;
    background-color: #000;
  }
  100% {
    opacity: 1;
  }
}

@keyframes contentFadeIn {
  0% {
    opacity: 0;
    transform: translateY(20px);
  }
  40% {
    opacity: 0;
  }
  100% {
    opacity: 1;
    transform: translateY(0);
  }
}

@keyframes comein {
  0% { letter-spacing: -5em; }
  100% { letter-spacing: -0.02em; }
}

@keyframes breathing {
  0%   { box-shadow: 0 0 6px rgba(255, 215, 170, 0.2); }
  50%  { box-shadow: 0 0 14px rgba(255, 215, 170, 0.4); }
  100% { box-shadow: 0 0 6px rgba(255, 215, 170, 0.2); }
}

@keyframes borderFlow {
  0% { background-position: 0% 50%; }
  50% { background-position: 100% 50%; }
  100% { background-position: 0% 50%; }
}

@keyframes floatQuote {
  0% { transform: translateY(0); }
  50% { transform: translateY(-6px) rotate(-0.2deg); }
  100% { transform: translateY(0) rotate(0); }
}

@keyframes compassSpin {
  from { transform: rotate(0deg); }
  to { transform: rotate(360deg); }
}

@keyframes compassSpinFast {
  from { transform: rotate(0deg); }
  to { transform: rotate(360deg); }
}

@keyframes waExcerptFocus {
  0%   { filter: blur(12px) brightness(1.20); transform: translateY(8px) scale(1.02); }
  70%  { filter: blur(1px)  brightness(1.05); transform: translateY(0)  scale(1); }
  100% { filter: none; transform: translateY(0) scale(1); }
}

@keyframes waFlicker {
  0%, 100% { opacity: 1; }
  48%      { opacity: 0.96; }
  50%      { opacity: 0.985; }
  52%      { opacity: 0.97; }
}

@keyframes bookFloat {
  0%   { transform: translateY(-25px) scale(1.3) rotate(0deg); }
  15%  { transform: translateY(-32px) scale(1.3) rotate(0.3deg); }
  30%  { transform: translateY(-28px) scale(1.3) rotate(-0.2deg); }
  50%  { transform: translateY(-35px) scale(1.3) rotate(0.1deg); }
  65%  { transform: translateY(-30px) scale(1.3) rotate(-0.3deg); }
  80%  { transform: translateY(-33px) scale(1.3) rotate(0.2deg); }
  100% { transform: translateY(-25px) scale(1.3) rotate(0deg); }
}

@keyframes stickerFloat {
  0%, 100% { 
    transform: scale(1.25) rotate(-3deg) translateY(-10px);
    filter: brightness(1.1) drop-shadow(0 0 8px var(--color-accent));
  }
  50% { 
    transform: scale(1.25) rotate(-1deg) translateY(-15px);
    filter: brightness(1.2) drop-shadow(0 0 15px var(--color-accent)) drop-shadow(0 0 30px var(--color-link));
  }
}

@keyframes tp-front-out {
  0%, 31% { opacity: 1; }
  38%, 100% { opacity: 0; }
}

@keyframes tp-line1-in {
  0%, 44% { opacity: 0; }
  52%, 100% { opacity: 1; }
}

@keyframes tp-line2-in {
  0%, 70% { opacity: 0; }
  80%, 100% { opacity: 1; }
}

@keyframes whisper-fade-in {
  0% { opacity: 0; transform: translateY(10px); }
  100% { opacity: 1; transform: translateY(0); }
}

@keyframes whisper-fade-in-b {
  0% { opacity: 0; transform: translateY(-10px); }
  100% { opacity: 1; transform: translateY(0); }
}

@keyframes tremble-subtle {
  0%   { transform: translate(0, 0); }
  25%  { transform: translate(0.5px, -0.5px); }
  50%  { transform: translate(-0.5px, 0.5px); }
  75%  { transform: translate(0.5px, 0.5px); }
  100% { transform: translate(-0.5px, -0.5px); }
}


/* ==============================
   BACKGROUND & PRESENTATION WRAPPER
============================== */

.user-css-presentation {
  background-image: url(https://www.worldanvil.com/i/6982189/render);
  background-size: cover;
  background-position: center;
  background-color: rgba(0, 76, 76, 0.9);
  background-blend-mode: multiply;
  justify-content: center;
  align-items: center;
  animation: fadeInFromBlack 2s ease-out;
}

.user-css-presentation .world-global-header {
  position: fixed;
  top: 0;
  height: 100px;
  z-index: 1000;
  background: url(https://www.worldanvil.com/i/6982210/render) center/cover no-repeat;
  background-color: rgba(0, 0, 0, 0.6);
  background-blend-mode: overlay;
  padding: 15px;
  display: flex;
  align-items: center;
  width: 100%;
  min-width: unset;
  margin: 0 auto;
  box-shadow: 0 2px 6px rgba(0,0,0,0.4);
}

.user-css.world-global-header h1,
.user-css.world-global-header h2,
.user-css.world-global-header p {
  color: var(--color-text, #f8f8f8);
  margin: 0;
  text-align: center;
  font-size: clamp(18px, 3vw, 32px);
}

.user-css-presentation .world-global-header .d-block {
  display: flex !important;
}

.user-css-presentation .row .world-content {
  text-align: center !important;
}

.user-css {
  background: linear-gradient(
      to bottom,
      rgba(0, 20, 40, 0.3) 0%,
      rgba(0, 30, 30, 0.4) 80%
    ),
    url(https://www.worldanvil.com/i/6982205/render);
  box-shadow: inset 0px 0px 20px var(--color-background);
  width: fit-content;
  max-width: 100%;
  justify-self: center;
  box-sizing: border-box;
  margin: clamp(20px, 12vw, 500px);
}

.user-css > *:not(.article-title) {
  animation: contentFadeIn 2.5s ease-out;
}


/* ==============================
   HOMEPAGE: BASE
============================== */

.user-css.page-world-homepage {
  background: transparent !important;
  box-shadow: none !important;
}

.user-css.page-world-homepage h5 a {
  font-family: var(--font-header);
  font-size: 35px;
  top: 20px;
  position: relative;
}

.user-css.page-world-homepage #cover img {
  width: 100%;
  height: auto;
  display: block;
  -webkit-mask-image:
    linear-gradient(to bottom, black 45%, transparent 100%),
    linear-gradient(to top, black 65%, transparent 100%),
    linear-gradient(to right, black 45%, transparent 100%),
    linear-gradient(to left, black 45%, transparent 100%);
  -webkit-mask-composite: destination-in;
  mask-image:
    linear-gradient(to bottom, black 45%, transparent 100%),
    linear-gradient(to top, black 65%, transparent 100%),
    linear-gradient(to right, black 45%, transparent 100%),
    linear-gradient(to left, black 45%, transparent 100%);
  mask-composite: intersect;
  opacity: 0.6;
  filter: blur(0.1px) brightness(90%);
}

.user-css.page-world-homepage h2 {
  font-family: var(--font-quote);
  text-transform: capitalize;
  font-weight: 400;
  letter-spacing: 2px;
  background: var(--color-header);
  -webkit-background-clip: text;
  -webkit-text-fill-color: transparent;
  -moz-background-clip: text;
  color: transparent;
  font-size: clamp(35px, 5vw, 90px);
  text-shadow: var(--color-header-shadow);
  text-align: center;
  border-bottom: 2px solid var(--color-border);
}

.user-css.page-world-homepage .user-custom-content-3 img.img-fluid.img-responsive,
.user-css.page-world-homepage .user-custom-content-3 img.img-responsive.img-fluid {
  box-shadow: none !important;
  filter: none !important;
}

.user-css.page-world-homepage .user-custom-content-3 img.img-fluid.img-responsive:hover,
.user-css.page-world-homepage .user-custom-content-3 img.img-responsive.img-fluid:hover {
  box-shadow: none !important;
  filter: none !important;
}


/* ==============================
   WHITE LABEL MARK
============================== */

.user-css-presentation .widget-white-label-mark {
  margin: 0 auto;
  font-family: var(--font-meta) !important;
  color: var(--color-text) !important;
  border: 2px solid var(--color-border);
  border-radius: 0 5px 5px 0;
  border-left: none;
}

.user-css-presentation .widget-white-label-mark a {
  margin: 0 auto;
  font-family: var(--font-meta) !important;
  font-size: 15px;
}


/* ==============================
   WORLD: TITLE + SUBTITLE
============================== */

.user-css .heading h1 {
  margin: 0;
  padding-top: 0.5em;
  color: transparent;
  line-height: 1.1;
}

.user-css .world-subtitle {
  display: inline-block;
  font-family: var(--font-quote);
  font-size: clamp(30px, 5vw, 200px);
  font-weight: 400;
  letter-spacing: 2px;
  line-height: 1.2;
  padding: 0.2em 0;
  text-align: center;
  background: var(--color-header);
  -webkit-background-clip: text;
  -webkit-text-fill-color: transparent;
  -moz-background-clip: text;
  color: transparent;
  text-shadow: var(--color-header-shadow);
}


/* ==============================
   WORLD: HIDE AUTHORS + TOC MENU
============================== */

.user-css .world-authors {
  display: none;
}

.user-css .table-of-contents .world-menu {
  display: none;
}


/* ==============================
   WORLD NAVIGATION
============================== */

.user-css-presentation .world-navigation-palette-trigger {
  background: none !important;
  display: inline-block;
  cursor: pointer;
}

.user-css-presentation .world-navigation-palette-trigger .fa-list {
  content: url(https://www.worldanvil.com/i/6982203/render);
  width: 150px;
  height: auto;
  display: inline-block;
  position: absolute;
  top: 40px;
  transition: transform 0.6s ease, filter 0.3s ease;
  animation: compassSpin 20s linear infinite;
}

.user-css-presentation .world-navigation-palette-trigger:hover .fa-list {
  animation: compassSpinFast 2s linear infinite;
  filter:
    drop-shadow(0 0 10px rgba(139, 69, 19, 0.4))
    drop-shadow(0 0 10px rgba(205, 127, 50, 0.3))
    drop-shadow(0 0 60px rgba(233, 170, 92, 0.5));
}

.user-css-presentation #world-navigation-sidebar {
  width: clamp(200px, 20%, 250px);
  max-height: 40vh;
  left: 150px;
  position: fixed;
  background: linear-gradient(0deg, rgba(4, 9, 15, 1) 0%, rgba(9, 42, 47, 1) 100%);
  border: 2px solid var(--color-border);
  box-shadow: inset 0 0 10px var(--color-muted);
  text-align: center;
  scrollbar-color: var(--color-muted) var(--color-background);
  border-top: 5px double var(--color-muted);
  border-radius: 15px;
  padding: 10px;
  overflow-y: auto;
  z-index: 1500;
}

.user-css-presentation #world-navigation-sidebar .map-box,
.user-css-presentation #world-navigation-sidebar .maps-list-header {
  display: none;
}

.user-css .world-meta-categories-link,
.user-css .world-menu,
.user-css .world-toc-header,
.user-css.page.sidebar-panel-open .world-timelines {
  display: none;
}

.user-css .world-navigation-custom-content h2 {
  font-family: var(--font-header);
  border-bottom: 3px double var(--color-border);
  background: var(--color-header);
  -webkit-background-clip: text;
  -webkit-text-fill-color: transparent;
  -moz-background-clip: text;
  color: transparent;
  font-size: clamp(24px, 4vw, 40px);
  text-align: center;
}

.user-css .world-navigation-custom-content a {
  display: block;
  line-height: 1.9em;
  font-weight: 400;
  font-size: clamp(14px, 4vw, 20px);
  font-family: var(--font-body);
  color: var(--color-link);
  transition: color 0.3s ease, transform 0.3s ease;
  text-decoration: none;
  margin-bottom: 5px;
}

.user-css .world-navigation-custom-content a:hover {
  color: var(--color-accent);
  transform: scale(1.05);
  text-decoration: underline;
}


/* ==============================
   LINKS
============================== */

.user-css a {
  color: var(--color-link);
  font-family: var(--font-body);
  font-size: clamp(16px, 1.2vw, 25px);
  text-decoration: none;
  transition: all 0.3s ease;
}

.user-css a:not(.btn):not(.world-navigation-custom-content a):hover {
  background: var(--color-link-hover);
  -webkit-background-clip: text;
  -webkit-text-fill-color: transparent;
  color: transparent;
  text-shadow: 0 0 8px var(--color-shadow);
}


/* ==============================
   SEARCH BAR
============================== */

.user-css .search-form .typeahead-input {
  z-index: 1200;
  position: fixed;
  top: calc(var(--header-height, 90px));
  width: 100%;
  left: -1px;
  text-align: center;
  background: var(--color-secondary-gradient);
  font-family: var(--font-header);
  font-size: clamp(18px, 2.5vw, 25px);
  color: var(--color-accent);
  letter-spacing: 2px;
  padding: clamp(5px, 3vw, 10px) 0 clamp(8px, 2vw, 5px);
  outline: none;
  transition: all 0.3s ease-in-out;
}

.user-css .search-form .typeahead-input:focus {
  animation: breathing 2s infinite ease-in-out;
  border-color: var(--color-accent);
}


/* ==============================
   BREADCRUMBS
============================== */

.user-css .article-title {
  display: flex;
  flex-direction: column;
}

.user-css .breadbox { order: 1; }
.user-css .article-title .m-b-0 { order: 2; }
.user-css .article-title-author { order: 3; }

.user-css .breadbox .breadcrumb-world-link-wrapper {
  display: none;
}

.user-css .breadbox {
  margin: 30px -2px 10px 15px;
}

.user-css .breadbox .breadcrumb {
  font-family: var(--font-header);
  font-size: 30px;
  letter-spacing: 2px;
  height: 80px;
  background: transparent;
}

.user-css .breadbox .breadcrumb a {
  font-family: var(--font-header);
  font-size: 30px;
  font-weight: 400;
  letter-spacing: 2px;
}

.user-css .category-contents .breadcrumb {
  display: none;
}

.user-css .breadcrumb-article-icon {
  color: var(--color-text);
}


/* ==============================
   ARTICLE HEADERS
============================== */

.article-title h1 {
  display: block;
  animation: comein 1.0s 1 ease-in-out forwards;
}

.user-css .article-content h1 {
  font-family: var(--font-header);
  text-transform: uppercase;
  font-size: clamp(32px, 6vw, 80px);
  text-align: center;
  background: var(--color-header);
  -webkit-background-clip: text;
  -webkit-text-fill-color: transparent;
  -moz-background-clip: text;
  color: transparent;
  margin-top: 40px;
}

.user-css .article-content h2 {
  font-size: clamp(24px, 4vw, 55px);
  border-bottom: 2px solid var(--color-link);
  font-family: var(--font-header);
  background: var(--color-small-header);
  -webkit-background-clip: text;
  -webkit-text-fill-color: transparent;
  -moz-background-clip: text;
  color: transparent;
  font-weight: 700;
  letter-spacing: 2px;
  text-transform: uppercase;
}

.user-css .article-content h3,
.user-css .article-content h4,
.user-css .article-content h5 {
  font-family: var(--font-header);
  background: var(--color-small-header);
  -webkit-background-clip: text;
  -webkit-text-fill-color: transparent;
  -moz-background-clip: text;
  color: transparent;
  font-weight: 700;
  letter-spacing: 2px;
  text-transform: uppercase;
}

.user-css .article-content h3 { font-size: clamp(22px, 3.5vw, 50px); }
.user-css .article-content h4 { font-size: clamp(20px, 3vw, 45px); }
.user-css .article-content h5 { font-size: clamp(18px, 2.5vw, 35px); }


/* ==============================
   ARTICLE CONTENT BASE
============================== */

.user-css .article-content {
  font-family: var(--font-body);
  color: var(--color-text);
  font-size: clamp(16px, 1.2vw, 25px);
  line-height: 1.6;
}

.user-css #cover img {
  -webkit-mask-composite: destination-in;
  mask-image:
    linear-gradient(to bottom, black 15%, transparent 100%),
    linear-gradient(to top, black 80%, transparent 100%);
  mask-composite: intersect;
  opacity: 0.7;
  filter: brightness(90%);
}


/* ==============================
   ARTICLE IMAGES
============================== */

.user-css img,
.user-css-extended img {
  max-width: 100%;
  height: auto;
  object-fit: contain;
}

.user-css .article-content .img-responsive.img-fluid,
.user-css .article-content .max-width-100 {
  max-width: 100%;
  height: auto;
  object-fit: contain;
}

.user-css .article-content .img-responsive.img-fluid:not(.bookcover img),
.user-css .article-content .max-width-100:not(.bookcover img) {
  display: block;
  height: auto;
  filter: sepia(1) contrast(1.1) brightness(0.7);
  -webkit-mask-image: radial-gradient(circle, rgba(0,0,0,1) 70%, rgba(0,0,0,0) 100%);
  border: 40px solid transparent;
  border-image: url(https://www.worldanvil.com/i/6986890/render) 30 round;
  transform: rotate(-0.8deg);
  transition: transform 0.6s ease, filter 4s ease, box-shadow 0.6s ease;
}

.user-css .article-content .img-responsive.img-fluid:not(.bookcover img):hover,
.user-css .article-content .max-width-100:not(.bookcover img):hover {
  box-shadow: inset 0 0 100px rgba(0,0,0,1);
  filter: sepia(0) contrast(1.1) brightness(1) opacity(0.9);
  transform: rotate(0.5deg) translateY(-4px);
}


/* ==============================
   ARTICLE PANELS
============================== */

.user-css .article-panel {
  position: relative;
  overflow: hidden;
  border-radius: 20px 0px;
  background: var(--color-secondary-bg);
  border: 1px solid var(--color-border);
  transition: box-shadow 1s ease;
}

.user-css .article-panel:hover {
  box-shadow: 0 0 15px var(--color-shadow);
  z-index: 10;
}

.user-css .article-panel .heading {
  font-family: var(--font-header);
  font-size: clamp(20px, 2vw, 35px);
  background: var(--color-header);
  -webkit-background-clip: text;
  -webkit-text-fill-color: transparent;
  -moz-background-clip: text;
  color: transparent;
  margin-bottom: 10px;
}

.user-css .article-panel .subheading {
  display: none;
}

.user-css .article-panel::before {
  content: close-quote;
  font-size: 0;
  line-height: 0;
  position: absolute;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  background-color: var(--color-background);
  opacity: 0;
  transition: opacity 0.4s ease;
  z-index: 2;
  pointer-events: none;
}

.user-css .article-panel .details,
.user-css .article-panel .details .heading {
  text-align: center;
}

.user-css .article-panel .details .heading {
  width: 100%;
  margin-left: auto;
  margin-right: auto;
}

.user-css .article-panel:hover::before {
  opacity: 1;
}

.user-css .article-panel .details p {
  position: absolute;
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
  background: var(--color-background-gradient);
  padding: 20px;
  margin: 0;
  overflow-y: auto;
  font-family: var(--font-body);
  font-size: clamp(16px, 2vw, 22px);
  line-height: 1.6;
  color: var(--color-link);
  text-shadow: 0 0 2px var(--color-accent);
  border-radius: 20px 0px;
  opacity: 0;
  transition: opacity 0.4s ease;
  z-index: 3;
  text-align: center;
  display: flex;
  align-items: center;
  justify-content: center;
}

.user-css .article-panel:hover .details p {
  opacity: 1;
}

.user-css .article-panel .details p.excerpt {
  opacity: 0 !important;
  filter: blur(12px) brightness(1.15);
  transform: translateY(8px) scale(1.02);
  pointer-events: none;
  transition: opacity 0.25s ease;
}

.user-css .article-panel:hover .details p.excerpt {
  opacity: 1 !important;
  animation:
    waExcerptFocus 1.1s ease forwards,
    waFlicker 3.2s ease-in-out infinite;
}


/* ==============================
   CREDITS CONTAINER
============================== */

.user-css .credits {
  color: var(--color-text);
  margin: 12px auto;
  padding: 8px 16px;
  font-family: var(--font-meta);
  font-size: clamp(12px, 1.2vw, 16px);
  line-height: 1.4;
  text-align: center;
  max-width: 100%;
  box-sizing: border-box;
  background-color: var(--color-background);
  border: 1px solid var(--color-border);
  border-radius: 8px;
  box-shadow: 0 2px 6px rgba(0,0,0,0.5);
  margin-left: auto;
  margin-right: auto;
  transition: box-shadow 0.3s ease, transform 0.3s ease;
}

.user-css .credits:hover {
  box-shadow: 0 2px 6px rgba(0, 0, 0, 0.5), 0 0 10px rgba(224, 200, 168, 0.4);
  transform: scale(1.02);
}

.user-css .credits a {
  font-family: var(--font-meta);
  font-size: clamp(12px, 1.2vw, 16px);
  text-decoration: none;
  transition: color 0.3s ease;
}


/* ==============================
   SOCIAL CONTAINER
============================== */

.user-css .social {
  position: absolute;
  top: 0;
  right: 0;
  display: flex;
  align-items: center;
  justify-content: flex-end;
  gap: 20px;
  padding: 10px 15px;
  font-family: var(--font-header);
  font-size: clamp(15px, 1.5vw, 25px);
  color: var(--color-link);
  text-shadow: 0 0 5px var(--color-shadow);
  background: var(--color-background);
  opacity: 0.5;
  pointer-events: auto;
  z-index: 10;
  height: 100px;
  width: 100%;
}

.user-css .social img {
  height: 28px;
  width: auto;
  transition: all 0.3s ease;
  cursor: pointer;
}

.user-css .social img:hover {
  transform: scale(1.4);
}


/* ==============================
   OVERLAY WRAPPER
============================== */

.user-css .overlay-wrapper {
  position: relative;
  width: 100%;
  min-height: 400px;
  overflow: hidden;
  background-color: var(--color-background);
}

.user-css .overlay-base {
  position: absolute;
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
  background-image: url(https://www.worldanvil.com/i/6982205/render);
  background-position: center;
  z-index: 1;
  mix-blend-mode: difference;
}

.user-css .overlay-top {
  position: relative;
  z-index: 2;
  box-sizing: border-box;
}


/* ==============================
   PRIMER CONTAINER
============================== */

.user-css .primer {
  position: relative;
  border-style: solid;
  border-width: 90px;
  border-image: url(https://www.worldanvil.com/i/6982193/render) 900;
  border-image-outset: 12px 7px;
  font-family: var(--font-body);
  font-size: clamp(16px, 1.5vw, 22px);
  color: var(--color-text);
  text-align: center;
  padding: 20px;
  box-sizing: border-box;
  z-index: 2;
}


/* ==============================
   ALOUD CONTAINER
============================== */

.user-css .aloud {
  position: relative;
  margin: 50px auto;
  padding: 160px;
  border-image-source: url(https://www.worldanvil.com/i/6982212/render);
  border-image-slice: 200 fill;
  border-image-repeat: round;
  border-image-width: 200px;
  font-family: var(--font-body);
  font-size: clamp(14px, 3.6vw, 25px);
  font-style: normal;
  color: var(--color-muted);
  text-shadow: var(--color-shadow);
  -webkit-mask-composite: destination-in;
  mask-image:
    linear-gradient(to bottom, black 65%, transparent 100%),
    linear-gradient(to top, black 75%, transparent 100%),
    linear-gradient(to right, black 95%, transparent 100%),
    linear-gradient(to left, black 95%, transparent 100%);
  mask-composite: intersect;
}


/* ==============================
   PARCHMENT CONTAINER
============================== */

.user-css .parchment {
  position: relative;
  background: linear-gradient(
    145deg,
    #b8a089 0%,
    #c9b89e 15%,
    #d4c4a8 30%,
    #c9b598 50%,
    #bda888 70%,
    #b09878 85%,
    #a68d6a 100%
  );
  color: #1a1410;
  font-family: var(--font-body);
  font-size: clamp(16px, 1.4vw, 22px);
  line-height: 1.8;
  padding: 40px 50px;
  margin: 30px auto;
  max-width: 90%;
  border: 3px solid var(--color-border);
  border-radius: 2px;
  box-shadow: 
    0 0 0 6px var(--color-background),
    0 0 0 8px rgba(180, 116, 77, 0.4),
    inset 0 0 60px rgba(80, 55, 30, 0.12),
    0 8px 30px rgba(0, 0, 0, 0.6);
}

.user-css .parchment::before {
  content: '';
  position: absolute;
  inset: 12px;
  border: 1px solid rgba(60, 40, 20, 0.2);
  border-radius: 0;
  pointer-events: none;
}

.user-css .parchment::after {
  content: '';
  position: absolute;
  inset: 0;
  background: 
    radial-gradient(ellipse at top left, rgba(60, 40, 20, 0.18) 0%, transparent 35%),
    radial-gradient(ellipse at bottom right, rgba(60, 40, 20, 0.2) 0%, transparent 35%),
    radial-gradient(ellipse at top right, rgba(60, 40, 20, 0.1) 0%, transparent 30%),
    radial-gradient(ellipse at bottom left, rgba(60, 40, 20, 0.1) 0%, transparent 30%);
  pointer-events: none;
  border-radius: 2px;
}

.user-css .parchment h2,
.user-css .parchment h3,
.user-css .parchment h4,
.user-css .parchment h5 {
  font-family: var(--font-header);
  color: #3d2410;
  -webkit-text-fill-color: #3d2410;
  background: none;
  text-shadow: 0 1px 0 rgba(255, 255, 255, 0.3);
  letter-spacing: 1px;
  border-bottom: 2px solid var(--color-border);
  padding-bottom: 8px;
  margin-bottom: 16px;
}

.user-css .parchment h2 { font-size: clamp(28px, 4vw, 48px); }
.user-css .parchment h3 { font-size: clamp(24px, 3vw, 38px); }
.user-css .parchment h4 { font-size: clamp(20px, 2.5vw, 32px); }
.user-css .parchment h5 { font-size: clamp(18px, 2vw, 26px); }

.user-css .parchment a {
  color: #5c3a1e;
  font-weight: 600;
  text-decoration: none;
  border-bottom: 1px solid var(--color-border);
  transition: all 0.3s ease;
}

.user-css .parchment a:hover {
  color: var(--color-link);
  border-bottom-color: var(--color-link);
  background: none;
  -webkit-text-fill-color: var(--color-link);
}


/* ==============================
   TREASURE PARCHMENT (ANIMATED CLUES)
============================== */

.user-css .treasure-parchment {
  background-image: url(https://wa-cdn.nyc3.digitaloceanspaces.com/user-data/production/c4e07060-742c-422d-972b-be3e8bccf927/uploads/images/05dc9ca04ddf4307f8563b32f659fd2e.png);
  background-repeat: no-repeat;
  background-size: contain;
  background-position: center center;
  width: min(100%, 920px);
  aspect-ratio: 3 / 4;
  margin: 24px auto;
  border-radius: 12px;
  display: flex;
  align-items: center;
  justify-content: center;
  padding: clamp(24px, 5vw, 64px);
  box-sizing: border-box;
  color: #2a1b10;
  text-align: center;
  text-shadow: 0 0 1px rgba(0, 0, 0, 0.25);
}

.user-css .treasure-parchment .tp-wrap {
  position: relative;
  width: 80%;
  height: 50%;
  display: flex;
  align-items: center;
  justify-content: center;
}

.user-css .treasure-parchment .tp-front,
.user-css .treasure-parchment .tp-back {
  position: absolute;
  top: 50%;
  left: 50%;
  width: 100%;
  font-family: var(--font-quote);
  font-size: clamp(28px, 4vw, 52px);
  line-height: 1.4;
  text-align: center;
}

.user-css .treasure-parchment .tp-front {
  color: #2a1b10;
  transform: translate(-40%, 25%) rotate(-1.5deg);
  opacity: 1;
  animation: tp-front-out 16s ease forwards;
}

.user-css .treasure-parchment .tp-back {
  transform: translate(-40%, 75%) rotate(-1.5deg);
}

.user-css .treasure-parchment .tp-line1 {
  color: #2a1b10;
  margin-bottom: 20px;
  opacity: 0;
  animation: tp-line1-in 16s ease forwards;
}

.user-css .treasure-parchment .tp-line2 {
  color: #5a1a12;
  font-style: italic;
  text-shadow: 0 0 5px rgba(48, 0, 0, 0.25);
  opacity: 0;
  animation: tp-line2-in 16s ease forwards;
}

.user-css .treasure-parchment .burn-wrapper {
  cursor: help;
}

.user-css .treasure-parchment .burn-wrapper:hover {
  cursor: pointer;
}


/* ==============================
   PAPER CONTAINER
============================== */

.user-css .paper {
  background: url(https://cdn.midjourney.com/ce4b395e-a158-486b-ab9a-1b01033ca757/0_0.png);
  position: relative;
  color: black;
  border: 10px solid #000000;
  background-size: cover;
  background-position: center;
  margin: 1px;
  border-radius: 0px 15px 0px 15px;
  padding: 100px;
}


/* ==============================
   BURN/QUOTE EFFECT (HOVER REVEAL)
============================== */

.user-css .burn-wrapper {
  position: relative;
  width: 100%;
  overflow: hidden;
}

.user-css .burn-front {
  position: relative;
  font-family: Georgia, serif;
  font-size: 30px;
  z-index: 2;
  opacity: 1;
  transition: opacity 2s ease, filter 2s ease, transform 2s ease;
}

.user-css .burn-front,
.user-css .burn-back {
  padding: 50px;
}

.user-css .burn-wrapper:hover .burn-front {
  opacity: 0;
  filter: blur(8px) brightness(1.4) sepia(1);
  transform: scale(1.02) rotate(-1deg);
}

.user-css .burn-back {
  position: absolute;
  top: 0;
  right: 0;
  left: 0;
  bottom: 0;
  z-index: 1;
  opacity: 0;
  transform: scale(1.03) rotate(2deg);
  transition: color 2s ease, text-shadow 2s ease, transform 1s ease;
}

.user-css .burn-wrapper:hover .burn-back {
  animation: whisper-fade-in 1.8s ease forwards, tremble-subtle 1s infinite;
  transform: scale(1.05) rotate(-10deg);
}

.user-css .burn-back .text1 {
  opacity: 0;
  font-family: Arial, sans-serif;
  font-size: 30px;
  color: white;
  animation: whisper-fade-in-b 1s ease forwards;
  animation-delay: 0.1s;
  animation-play-state: paused;
}

.user-css .burn-back .text2 {
  opacity: 0;
  font-family: Courier New, monospace;
  font-size: 28px;
  margin-top: 20px;
  color: #aa0000;
  text-shadow: 0 0 5px #300000;
  animation: whisper-fade-in 1s ease forwards;
  animation-delay: 2s;
  animation-play-state: paused;
}

.user-css .burn-wrapper:hover .burn-back .text1 {
  animation-play-state: running;
  animation-name: whisper-fade-in;
}

.user-css .burn-wrapper:hover .burn-back .text2 {
  animation-play-state: running;
  animation-name: whisper-fade-in-b;
}


/* ==============================
   BLOCKQUOTES
============================== */

.user-css blockquote,
.user-css-presentation .tooltipster-content blockquote {
  display: block;
  max-width: 900px;
  margin: 0.5em auto;
  padding: 30px 40px 20px;
  background: var(--color-background-gradient);
  border-radius: 20px;
  border: 2px solid var(--color-border);
  font-family: var(--font-quote);
  font-size: clamp(18px, 2.5vw, 35px);
  color: var(--color-link);
  line-height: 1.6;
  text-align: center;
  box-shadow: 0 0px 10px var(--color-accent);
  transition: all 1s ease;
  animation: floatQuote 4s ease-in-out infinite;
}

.user-css blockquote a {
  font-size: clamp(18px, 2.5vw, 35px);
  color: var(--color-accent);
  font-family: var(--font-quote);
}

.user-css blockquote:hover {
  box-shadow: inset 0 0 20px var(--color-shadow);
  rotate: 1deg;
}

.user-css blockquote .author {
  font-family: var(--font-meta);
  font-size: 14px;
  color: var(--color-muted);
  opacity: .7;
  font-style: normal;
  text-align: right;
}

.user-css blockquote .author a {
  font-family: var(--font-meta);
  font-size: 14px;
}


/* ==============================
   HR
============================== */

.user-css hr {
  border: none;
  border: 10px solid;
  border-image-source: url(https://wa-cdn.nyc3.digitaloceanspaces.com/user-data/production/c4e07060-742c-422d-972b-be3e8bccf927/uploads/images/f7abbea934351f3a0e714026d471734d.webp);
  border-image-slice: 60 fill;
  height: 100px;
  border-image-repeat: round;
  border-image-width: 70px;
  margin: 100px auto;
  background: none;
  opacity: 0.8;
}


/* ==============================
   BACK TO WORLD BUTTON
============================== */

.user-css .backtoworld a {
  display: inline-block;
  padding: 12px 24px;
  font-family: var(--font-quote);
  font-size: clamp(16px, 2.5vw, 30px);
  letter-spacing: 1.5px;
  color: var(--color-link);
  background: var(--color-background);
  border: 2px solid transparent;
  border-radius: 12px;
  position: relative;
  text-decoration: none;
  transition: all 2s ease;
  overflow: hidden;
  z-index: 1;
}

.user-css .backtoworld a::before {
  content: close-quote;
  position: absolute;
  inset: 0;
  border-radius: 12px;
  padding: 2px;
  background: linear-gradient(
    135deg,
    #3b2a20, #6e4b2f, #9c7744, #c6a56d, #e6d5b8, #c6a56d, #9c7744, #6e4b2f
  );
  background-size: 300% 300%;
  animation: borderFlow 6s ease infinite;
  -webkit-mask: linear-gradient(#fff 0 0) content-box, linear-gradient(#fff 0 0);
  -webkit-mask-composite: xor;
  mask-composite: exclude;
  z-index: -1;
}

.user-css .backtoworld a:hover {
  transform: scale(1.05);
  color: var(--color-accent);
  box-shadow: 0 0 15px rgba(224, 200, 168, 0.4);
  rotate: -2deg;
}


/* ==============================
   DROPCAP
============================== */

.user-css .dropcap {
  font-family: var(--font-quote);
  font-size: 150px;
  margin-right: 5px;
}



/* "While digging for buried treasure, your shovel suddenly hits something solid. Something made of Wood. Clearing away the sand, you discover an old wooden chest. Upon opening it, you find a single dusty scroll." - (1ee1db34-eecf-4c80-b9f1-4d15cc280296) */


/* ==============================
   USERCARD
============================== */

.user-css .usercard {
  background-color: var(--color-background);
  border: 2px solid var(--color-border);
  border-radius: 10px;
  color: var(--color-text);
  font-family: var(--font-body);
  line-height: 2;
  padding: 30px 30px 0px;
  box-shadow: inset 0px 0px 10px var(--color-border);
}


/* ==============================
   ARTICLE HORIZONTAL NAVIGATION
============================== */

.user-css .article-horizontal-navigation a {
  font-family: var(--font-quote);
  font-size: 45px;
  letter-spacing: 2px;
}


/* ==============================
   MAP BOXES
============================== */

.user-css .map-box {
  width: clamp(300px, 40%, 700px);
  display: inline-block;
  background: url(https://www.worldanvil.com/i/6982188/render) center/cover no-repeat;
  border: 2px solid var(--color-border);
  border-radius: 15px;
  height: auto;
  min-height: 300px;
  margin: 30px;
  margin-left: 60px;
  padding: 0;
  box-sizing: border-box;
  color: var(--color-text);
}

.user-css .map-box .p-20 {
  padding: 0 !important;
  margin: 0 auto;
  height: 90px;
  background: var(--color-background);
  opacity: 0.8;
  transform: translateY(-5px);
}

.user-css .map-box h5 {
  font-family: var(--font-header);
  font-size: clamp(20px, 3vw, 24px);
  font-weight: 700;
  background: var(--color-header);
  -webkit-background-clip: text;
  -webkit-text-fill-color: transparent;
  -moz-background-clip: text;
  color: transparent;
  text-shadow: rgba(0,0,0,0.3) 1px 1px 2px;
  transform: translateY(5px);
  text-align: center;
  letter-spacing: 1px;
}

.user-css .map-box .img-responsive.img-fluid {
  opacity: 0;
  border-radius: 12px 12px 0 0;
  transition: opacity 0.5s ease;
  pointer-events: none;
}

.user-css .map-box:hover .img-responsive.img-fluid {
  opacity: 1;
  pointer-events: auto;
}

.user-css .map-box:hover {
  transform: scale(1.02);
  box-shadow: 0 0 15px rgba(217, 142, 95, 0.4);
}


/* ==============================
   BOOK COVERS
============================== */

.user-css .bookcover {
  background: transparent;
  text-align: center;
  margin: 0 auto;
  display: inline-block;
  width: 300px;
  position: relative;
  height: 400px;
  overflow: visible;
  z-index: 1;
  transition: z-index 0s 0.4s;
}

.user-css .bookcover:hover {
  z-index: 100;
  transition: z-index 0s;
}

.user-css .bookcover img {
  display: inline-block;
  object-fit: contain;
  max-width: 100%;
  height: auto;
  opacity: 1;
  filter: brightness(70%);
  box-shadow: none !important; 
  border: none !important;
  transform: translateY(0) scale(1);
  transition: transform 0.5s cubic-bezier(0.34, 1.56, 0.64, 1), filter 0.4s ease;
}

.user-css .bookcover::before {
  content: '';
  position: absolute;
  bottom: -30px;
  left: 50%;
  transform: translateX(-50%);
  width: 0%;
  height: 0px;
  background: radial-gradient(ellipse, rgba(0,0,0,0.5), transparent);
  opacity: 0;
  transition: all 0.5s ease;
  z-index: -1;
}

.user-css .bookcover:hover::before {
  width: 60%;
  height: 15px;
  opacity: 0.3;
  filter: blur(10px);
  bottom: -60px;
}

.user-css .bookcover:hover img {
  filter: brightness(1)
          drop-shadow(0 0 20px rgba(233, 170, 92, 0.7))
          drop-shadow(0 0 40px rgba(205, 127, 50, 0.5))
          drop-shadow(0 0 60px rgba(139, 69, 19, 0.4));
  animation: bookFloat 3s ease-in-out infinite;
}

.user-css .booktitle {
  font-family: var(--font-quote);
  text-transform: capitalize;
  background: linear-gradient(
    45deg,
    #4A2C1A 0%,
    #A85C32 15%,
    #D98E5F 65%,
    #A85C32 85%,
    #4A2C1A 95%
  );
  -webkit-background-clip: text;
  -webkit-text-fill-color: transparent;
  -moz-background-clip: text;
  color: transparent;
  font-size: clamp(35px, 5vw, 32px);
  font-weight: 400;
  letter-spacing: 1px;
  text-shadow: var(--color-shadow);
  position: relative;
  top: 0;
  left: 0;
}

.user-css .bookcover .excerpt {
  display: none;
}


/* ==============================
   BUTTONS
============================== */

.user-css .btn-success:focus,
.user-css .btn-success:active,
.user-css .btn-primary:focus,
.user-css .btn-primary:active,
.user-css .btn-info:focus,
.user-css .btn-info:active,
.user-css .btn-danger:focus,
.user-css .btn-danger:active {
