  box-shadow: none !important;
}

.user-css .btn-primary,
.user-css .btn-success {
  background: linear-gradient(145deg, #0d2f2f, #081a1a);
  border: 1px solid #0f3d46 !important;
  padding: 10px 20px 5px;
  border-radius: 8px;
  font-weight: 400;
  font-family: var(--font-meta);
  font-size: 20px;
  transition: all 0.3s ease-in-out;
}

.user-css .btn-primary:hover {
  background: linear-gradient(145deg, #124444, #0b2222);
  box-shadow: inset 0 0 10px rgba(168, 255, 222, 0.4),
              0 3px 8px rgba(0, 0, 0, 0.8);
  transform: translateY(-2px);
}

.user-css .primer .btn-success {
  background-color: var(--color-secondary-bg) !important;
  border: 1px solid var(--color-border) !important;
  color: var(--color-link) !important;
  font-family: var(--font-quote);
  font-size: clamp(16px, 2.5vw, 30px);
  letter-spacing: 1.5px;
  transition: transform 2s ease, filter 0.3s ease, border 0.3s ease, color 1s ease;
}

.user-css .primer .btn-success:hover {
  transform: scale(1.1);
  border: 0px solid transparent !important;
  background: var(--color-link-hover);
  -webkit-background-clip: text;
  -webkit-text-fill-color: transparent;
  color: transparent;
  box-shadow: none;
}

.user-css .btn-secondary {
  font-family: var(--font-meta);
  font-size: 20px;
  color: var(--color-text);
  font-weight: 400;
  padding: 10px;
  border-radius: 8px;
  border: 1px solid var(--color-border);
  transition: all 0.4s ease;
  cursor: pointer;
  position: relative;
  overflow: hidden;
  background: linear-gradient(135deg, #04131d, #092a2f);
  border: 1px solid #0f3d46;
}

.user-css .btn-secondary:hover {
  background: linear-gradient(135deg, #092a2f, #04131d);
  transform: translateY(-2px);
}


/* ==============================
   STICKERS, LIKES, READING LIST
============================== */

.user-css .btn-danger,
.user-css .btn-info,
.user-css .btn-sticker,
.user-css #likes-indicator.text-danger {
  font-family: var(--font-meta);
  font-size: 20px;
  color: var(--color-text);
  font-weight: 400;
  padding: 10px;
  border-radius: 8px;
  border: 1px solid var(--color-border);
  transition: all 0.4s ease;
  cursor: pointer;
  position: relative;
  overflow: hidden;
}

.user-css #likes-indicator.text-danger {
  background: linear-gradient(135deg, #04131d, #092a2f) !important;
  color: var(--color-text) !important;
  font-family: var(--font-meta) !important;
